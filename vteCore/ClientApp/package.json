    "@tanstack/react-query": "^4.35.3",
    "axios": "^1.3.4",
    "bulma": "^0.9.4",
    "clsx": "^2.0.0",
    "dayjs": "^1.11.10",
    "fast-sort": "^3.4.0",
    "history": "^5.3.0",
    "mantine-react-table": "^1.3.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
